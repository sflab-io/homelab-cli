{
  "name": "homelab-cli",
  "description": "My homelab CLI",
  "version": "0.3.4",
  "author": "Sebastian Freund",
  "bin": {
    "homelab": "./bin/run.js"
  },
  "bugs": "https://github.com/sflab-io/homelab-cli/issues",
  "dependencies": {
    "@oclif/core": "^4",
    "@oclif/plugin-help": "^6",
    "@oclif/plugin-plugins": "^5",
    "cli-table3": "^0.6.5",
    "configstore": "^7.1.0",
    "date-fns": "^4.1.0",
    "enquirer": "^2.4.1",
    "execa": "^9.6.0",
    "proxmox-api": "^1.1.1",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@eslint/compat": "^1",
    "@oclif/prettier-config": "^0.2.1",
    "@oclif/test": "^4",
    "@types/chai": "^4",
    "@types/configstore": "^6.0.2",
    "@types/mocha": "^10",
    "@types/node": "^18",
    "@types/sinon": "^17.0.4",
    "chai": "^4",
    "eslint": "^9",
    "eslint-config-oclif": "^6",
    "eslint-config-prettier": "^10",
    "mocha": "^10",
    "oclif": "^4",
    "shx": "^0.3.3",
    "sinon": "^21.0.0",
    "ts-node": "^10",
    "tsx": "^4.20.6",
    "typescript": "^5"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
    "./bin",
    "./dist",
    "./oclif.manifest.json"
  ],
  "homepage": "https://github.com/sflab-io/homelab-cli",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "type": "module",
  "oclif": {
    "bin": "homelab",
    "dirname": "homelab",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-plugins"
    ],
    "topicSeparator": " ",
    "topics": {}
  },
  "repository": "sflab-io/homelab-cli",
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "pnpm run lint",
    "prepack": "oclif manifest && oclif readme",
    "test": "mocha --forbid-only --exit \"test/**/*.test.ts\"",
    "version": "oclif readme && git add README.md"
  },
  "types": "dist/index.d.ts"
}
